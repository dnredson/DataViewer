    <script>
function drawChart(dataArray) {
  
  
        
        var x = dataArray.index;
        console.log("Array");
        console.log(dataArray);
        var moisture1 = [];
        dataArray.attributes.forEach((element) => {
          if (element.attrName == "soilMoistureCalibratedDepth1") {
            element.values.forEach((valor) => {
              if (valor != null){
              moisture1.push(valor);
              }else{
                moisture1.push(0);
              }
            });
          }
        });
        var moisture2 = [];
        dataArray.attributes.forEach((element) => {
          if (element.attrName == "soilMoistureCalibratedDepth2") {
            element.values.forEach((valor) => {
              
              moisture2.push(valor);
            });
          }
        });
        var moisture3 = [];
        dataArray.attributes.forEach((element) => {
          if (element.attrName == "soilMoistureCalibratedDepth3") {
            element.values.forEach((valor) => {
              
              moisture3.push(valor);
            });
          }
        });
       
        
        var optionsMoisture = {
          title: {
            text: "Moisture Calibrated",
            align: "left",
          },
          dataLabels: {
            enabled: true,
          },
          chart: {
            type: "line",
            height: 350,
          },
          series: [
            {
              name: "soilMoistureCalibratedDepth1",
              data: moisture1,
            },{
              name: "soilMoistureCalibratedDepth1",
              data: moisture2
            },{
              name: "soilMoistureCalibratedDepth1",
              data: moisture3
            }
          ],
          xaxis: {
            type: "datetime",
            categories: x,
          },
        };
        var divmoisture = dataArray.entityId+"moisture";
        
        var chart = new ApexCharts(
         
          document.getElementById(divmoisture),
          optionsMoisture
        );
        chart.render();

           
      }

    </script>
